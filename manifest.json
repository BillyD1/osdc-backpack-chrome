{	"name"					: "OSDC Backpack"
, "version"				: "0.1"
,	"permissions"			:
		[ "tabs"
		, "contentSettings"
		, "contextMenus"
		, "debugger"
		]
,	"minimum_chrome_version": "16.0"
,	"manifest_version"		: 2
,	"homepage_url"			: "https://github.com/osdc/osdc-backpack-chrome"
,	"description"			: "OSDC backpack"

,	"icons"					:
		{ "16"				: "icon.png"
		, "48"				: "icon-48.png"
		, "128"				: "icon-128.png"
		}
,	"browser_action"		:
		{ "default_icon"	: "icon.png"
		}

,	"incognito"				: "spanning"
,	"background"			: 
		{ "page": "background.html"
		}
,	"options_page"			: "options.html"
}

